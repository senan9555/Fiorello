    <script>

        $(document).on ("keyup", "#my-search", function () {
            if($("#my-search").val().length>0){
               $("#my-list li").slice(1).remove()
               $.ajax.({
                   url: "/Home/MySearch/",
                   type:"get",
                   data:{
                    "key": $("#my-search").val()
                    },
                   success: function (response) {
                    $("#my-list").append(response)
                   }
               });
            }
            else{
                $("#my-list li").slice(1).remove()
            }
        });
    </script>
   
